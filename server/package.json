{
  "name": "music-web-app-backend",
  "version": "1.0.0",
  "description": "Backend project of Music-Web-App",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/nodemon/bin/nodemon.js src/app.ts",
    "build": "tsc",
    "test": "jest"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "keywords": [
    "noframework",
    "typescript",
    "music",
    "app"
  ],
  "author": "Caglar Alkis",
  "license": "ISC",
  "dependencies": {
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/formidable": "^1.0.31",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.0.10",
    "formidable": "^1.2.1",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  }
}
